        <addForeignKeyConstraint baseColumnNames="test_paper_id_id"
                                 baseTableName="testresult"
                                 constraintName="fk_testresult_test_paper_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="testpaper"/>

    </changeSet>
</databaseChangeLog>
