<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Testpaper.
    -->
    <changeSet id="20200930095452-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="enterprise_id"
                                 baseTableName="testpaper"
                                 constraintName="fk_testpaper_enterprise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enterprise"/>

        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="testpaper"
                                 constraintName="fk_testpaper_course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

        <addForeignKeyConstraint baseColumnNames="subject_id"
                                 baseTableName="testpaper"
                                 constraintName="fk_testpaper_subject_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>

        <addForeignKeyConstraint baseColumnNames="testpaper_id"
                                 baseTableName="testpaper_topic"
                                 constraintName="fk_testpaper_topic_testpaper_id"
                                 referencedColumnNames="id"
                                 referencedTableName="testpaper"/>

        <addForeignKeyConstraint baseColumnNames="topic_id"
                                 baseTableName="testpaper_topic"
                                 constraintName="fk_testpaper_topic_topic_id"
                                 referencedColumnNames="id"
                                 referencedTableName="topic"/>
        
    </changeSet>
</databaseChangeLog>
